{
  "name": "phpset/phpset",
  "description": "Flexible Set of fast PHP packages. Instead Framework.",
  "require": {
    "php": "^7.1",
    "ext-mbstring": "*",
    "psr/log": "^1.0",
    "psr/cache": "^1.0",
    "psr/container": "^1.0",
    "psr/http-message": "^1.0",
    "roave/security-advisories": "dev-master",
    "league/container": "*",
    "twig/twig": "*",
    "symfony/console": "*",
    "oscarotero/middleland": "0.4.0",
    "guzzlehttp/psr7": "*",
    "phpset/fastroute-middleware": "dev-master"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../fastroute-middleware"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}